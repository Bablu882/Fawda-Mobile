{
  "cli": {
    "version": ">= 3.8.1"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    "preview": {
      "distribution": "internal",
       "android": {
        "buildType": "apk",
        "extraPackageJson": {
          "expo": {
            "modules": ["expo-linking"]
          }
        }
        },
      "ios": {
        "resourceClass": "m-medium"
      }
    },
    
    "production": {
      "android": {
        "buildType":"aab",
        "extraPackageJson": {
          "expo": {
            "modules": ["expo-linking"]
          }
        }
      },
      "ios": {
        "resourceClass": "m-medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
